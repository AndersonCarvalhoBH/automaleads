datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Lead {
  id        String   @id @default(uuid())
  name      String
  email     String
  phone     String?
  status    String   @default("new")
  createdAt DateTime @default(now())
}
